# Device Onboarding Configuration
# Copy this file to .env and fill in your values

# Public URL reachable by routers (use your domain in production)
APP_PUBLIC_URL=http://localhost:5000

# Secret key for device callbacks (change this in production!)
CALLBACK_SECRET=supersecret_change_me_in_production

# Firebase Configuration (if not using service account file)
# GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account-key.json

# Server Configuration
PORT=5000
NODE_ENV=development

# Twilio Verify API Configuration
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_VERIFY_SERVICE_SID=your_twilio_verify_service_sid